// @lang=vli @ts=2

completeness readserial;
disable iff: (!reset_n); 
inputs: reset_n, rxd;

determination_requirements:
  determined(valid);
  if (valid) determined(data); endif;

reset_property:

  sva/inst_readserial_property_suite/ops/a_reset;
property_graph:
  sva/inst_readserial_property_suite/ops/a_reset -> sva/inst_readserial_property_suite/ops/a_read_byte;
	sva/inst_readserial_property_suite/ops/a_reset -> sva/inst_readserial_property_suite/ops/a_stay_in_idle;

  sva/inst_readserial_property_suite/ops/a_read_byte -> sva/inst_readserial_property_suite/ops/a_read_byte;
	sva/inst_readserial_property_suite/ops/a_read_byte -> sva/inst_readserial_property_suite/ops/a_stay_in_idle;

	sva/inst_readserial_property_suite/ops/a_stay_in_idle -> sva/inst_readserial_property_suite/ops/a_stay_in_idle;
	sva/inst_readserial_property_suite/ops/a_stay_in_idle -> sva/inst_readserial_property_suite/ops/a_read_byte;

end completeness;
